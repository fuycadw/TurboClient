<preference-headers
        xmlns:unified="http://schemas.android.com/apk/res-auto">

    <!--
	/*
	** Header definitions are identical to android:* except for preferenceRes which should be a reference to a preference xml file
	** Unlike the native headers these are also used for building the single pane version.
	*/
    -->
    <header
            unified:fragment="turbo.client.preference.PreferenceOfApplication$Ui"
            unified:title="Ui"
            unified:preferenceRes="@xml/pref_ui"/>
    <header
            unified:fragment="turbo.client.preference.PreferenceOfApplication$Editor"
            unified:title="Editor"
            unified:preferenceRes="@xml/pref_editor"/>
    <header
            unified:fragment="turbo.client.preference.PreferenceOfApplication$Altro"
            unified:title="@string/altro"
            unified:preferenceRes="@xml/pref_altro"/>

</preference-headers>